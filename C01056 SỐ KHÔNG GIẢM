#include <stdio.h>
#include <string.h>
int main()
{
	int test;
	scanf("%d", &test);
	while(test--)
	{
	char a[18];
	scanf("%s", &a);
		int dem=0;

	for (int i=0; i<= (strlen(a)-2);i++)
	{
		if(a[i]>a[i+1])
		{
			dem=1;
			printf("NO\n");
			break;
			}
			}
         if(dem==0)
			{
				printf("YES\n");
			}

}
	return 0;
}
