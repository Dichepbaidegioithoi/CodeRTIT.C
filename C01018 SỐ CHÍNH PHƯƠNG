
 #include <stdio.h>
 #include <string.h>
 #include <stdlib.h>
 #include <math.h>
 #include <ctype.h>
 int check_1(int n){
 	for(int i=1;i<=n;i++){
 		if(i*i==n){
 			return 1;
		 }
	 }
	 return 0;
 }
int main(){
	int t;
	scanf("%d",&t);
	while(t--){
		int n;
		scanf("%d",&n);
	if(check_1(n)==1){
		printf("YES\n");
	}
	else{
		printf("NO\n");
	}
	}
	return 0;
}
	
