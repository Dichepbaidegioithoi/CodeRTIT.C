#include <stdio.h>
 #include <string.h>
 #include <stdlib.h>
 #include <math.h>
 #include <ctype.h>
int main(){
	int t;
	scanf("%d",&t);
	while(t--){
		int n;
		scanf("%d",&n);
		int cnt=0;
		int idx=0;
		while(n!=0){
			int rem=n%2;
			if(rem%2==0){
				cnt++;
			}
		else{
			idx++;
		}
			n/=10;
		}
		printf("%d ",idx);
			printf("%d\n",cnt);
	}
	return 0;
}
	
