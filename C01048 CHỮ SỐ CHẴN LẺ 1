#include <stdio.h>
#include <math.h>
int main()
{
	int n;
	scanf("%d", &n);
	int dem1=0;
	int dem2=0;
	while (n!=0)
	{
   for (int i=1;i<=n;i++)
   {
   	int a=n%10;
   	if(a%2==1)
   	{
   		dem1++;
	   }
	   else if(a%2==0)
	   {
	   	dem2++;
	   }
	   else
	   {
	   	printf("0");
  }
	   n=n/10;
   }
}
   	printf("%d %d",dem1,dem2);
   return 0;
}
