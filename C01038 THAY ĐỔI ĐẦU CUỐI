#include <stdio.h>
#include <string.h>
int main()
{
	char a[10] ;
	scanf("%s", &a);
	if (a[strlen(a)-1] == '0')
	{
		for (int i=1; i<= (strlen(a)-2);i++)
	printf("%c", a[i]);
		printf("%c", a[0]);
	}
	else{
	
	printf("%c", a[strlen(a)-1]);
	for (int i=1; i<= (strlen(a)-2);i++)
	printf("%c", a[i]);
		printf("%c", a[0]);
	}
		return 0;

}
