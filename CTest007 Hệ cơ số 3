#include <stdio.h>
#include <string.h>
int main()
{
	int test;
	scanf("%d",&test);
	while(test--)
	{
	char a[18];
	scanf("%s",&a);
	int check=0;
	for(int i=0;i<strlen(a);i++)
	{
		if(a[i]=='2' || a[i]=='0' || a[i]=='1')
		{
			check=0;
		}else
		{
			check=1;
		}
	}
	if(check==0)
	{
		printf("YES\n");
	}else if(check==1)
	{
		printf("NO\n");
	}
}
	return 0;
}
